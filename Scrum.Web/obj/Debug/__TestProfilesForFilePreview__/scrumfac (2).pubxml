<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://scrumfac.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>scrumfac.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>scrumfac</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$scrumfac</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="ScrumWebContext" Order="1" Enabled="False">
          <Destination Path="Database=Scrum;Server=localhost;UserId=root;Password=1234" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="Scrum.Web.Models.ScrumWebContext, Scrum.Dominio" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)ScrumWebContext-Web.config Connection String">
      <ParameterValue>Database=Scrum;Server=localhost;UserId=root;Password=1234</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>